{
  "compilerOptions": {
    "target": "es2019",
    "module": "Node16",
    "moduleResolution": "Node16",
    "pretty": true,
    "declaration": true,
    "declarationMap": true,
    "strict": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": false,
    "types": ["node", "jest"]
  }
}
